import albumsSystem.albums.Album
import antidote.crdts.tables.UWTable
import antidote.crdts.lemmas.CvRDTProof1

class AlbumsTable[Time](before: (Time, Time) =>
                   Boolean, elements: Map[String, Tuple[Album, MVRegister[Int, Time]]])
                    extends UWTable[Album, Time, AlbumsTable[Time]] {

  def copy(newElements: Map[String, Tuple[Album, MVRegister[Int, Time]]]) =
    new AlbumsTable(this.before, newElements)

  def maintainState() = this
}

object AlbumsTable extends CvRDTProof1[AlbumsTable]