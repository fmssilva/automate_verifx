import albumsSystem.artists.Artist
import antidote.crdts.tables.UWTable
import antidote.crdts.lemmas.CvRDTProof1

class ArtistsTable[Time](before: (Time, Time) => Boolean,
                        elements: Map[String, Tuple[Artist, MVRegister[Int, Time]]])
                        extends UWTable[Artist, Time, ArtistsTable[Time]] {

  def copy(newElements: Map[String, Tuple[Artist, MVRegister[Int, Time]]]) =
    new ArtistsTable(this.before, newElements)

  def maintainState() = this

}

object ArtistsTable extends CvRDTProof1[ArtistsTable]