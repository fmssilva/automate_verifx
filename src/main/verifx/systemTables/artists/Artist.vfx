import antidote.crdts.lemmas.CvRDT
import antidote.crdts.lemmas.CvRDTProof

/**
 * Class representing the element:   ARTIST 
 * given by the Antidote SQL command:
 *		CREATE UPDATE-WINS TABLE ARTIST (
 *		NAME VARCHAR
 *		COUNTRY VARCHAR
 *		PRIMARY KEY ( NAME , COUNTRY )
 *		)
 */
class Artist(name: String, country: String) extends CvRDT[Artist] {


	//Implementation of methods DECLARED in CvRDT trait

	def merge(that: Artist) = 
		new Artist(this.name, this.country)

	def compare(that: Artist) = 
		true


	//Override Methods IMPLEMENTED in CvRDT trait

	override def compatible(that: Artist) = 
		this.name == that.name &&
		this.country == that.country

}
